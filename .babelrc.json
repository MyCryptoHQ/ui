{
  "presets": ["@babel/preset-typescript", "@babel/preset-react"],
  "comments": false,
  "ignore": ["**/*.d.ts", "**/*.test.ts", "**/*.test.tsx", "**/*.story.tsx"],
  "env": {
    "cjs": {
      "plugins": ["@babel/plugin-transform-modules-commonjs"]
    },
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}
